{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:3000",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach with sourceMaps",
      "port": 9222,
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///C:*":"C:/*"
      },
      "webRoot": "${workspaceRoot}/src"
      //,"userDataDir": "${workspaceRoot}/.vscode/chrome"
    }
  ],
  "compounds": []
}